<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.fav_stars">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".rayanrenolds"
                  android:label="Rayan Reynold"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".robertdowney"
                  android:label="Robert Downey Jr"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".quentintarantino"
                  android:label="Quentin Tarantino"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".margot"
                  android:label="Margot Robbie"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".keanureeves"
                  android:label="Keanu Reeves"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".johnneydeep"
                  android:label="Johnny Deep"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>